# Arquivo de exemplo para variáveis de ambiente
# Copie este arquivo para .env e preencha com seus valores

# Configurações do Servidor
PORT=3000
NODE_ENV=production

# Chave secreta para assinar os tokens JWT (use generatekeys.py para criar uma)
JWT_SECRET=SUA_CHAVE_SECRETA_LONGA_E_ALEATORIA_AQUI

# Credenciais da API Sankhya
SANKHYA_API_URL=https://api.sankhya.com.br
SANKHYA_APPKEY=CHAVE_DO_APLICATIVO_SANKHYA
SANKHYA_USERNAME=USUARIO_DA_API_SANKHYA
SANKHYA_PASSWORD=SENHA_DO_USUARIO_DA_API
SANKHYA_TOKEN=TOKEN_DO_APLICATIVO_SANKHYA

API_ONLY_MODE=true